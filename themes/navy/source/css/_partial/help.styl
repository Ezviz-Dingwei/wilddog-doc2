.help-top
	position: relative
	overflow: hidden
	margin-top: 80px
	padding-top :130px
	padding-bottom: 180px
	background-color: #f5f5f5
	.help-title
		position: relative
		z-index: 9
		padding-bottom: 50px
		font-size: 40px
		color: #333
		font-weight: 300
	.help-search
		position: relative
		z-index: 9
		margin-left: auto
		margin-right: auto
		width: 680px
		.help-search-input
			padding-left: 46px
			padding-right: 110px
			width: 100%
			height: 50px
			font-size: 16px
			outline: none
			border: 1px solid #ccc
			border-radius: 3px
		.help-search-btn
			position: absolute
			right: 0
			top: 0
			padding-left: 10px
			width: 100px
			height: 50px
			letter-spacing: 10px
			font-size: 16px
			cursor: pointer
			border-radius: 0 3px 3px 0
		.search-icon
			position: absolute
			top: 16px
			left: 20px
			box-shadow: none
			border: 0
	.rotate-circle-content
		position: absolute
		bottom: 0
		left: 50%
		margin-left: -635px
		margin-bottom: -550px
		width: 1270px
		height: 1270px
		.rotate-shadow
			position: absolute
			bottom: 760px
			left: 0
			z-index: 2
			width: 100%
			img
				box-shadow: none
				border: 0
		.rotate-circle
			position: absolute
			left: 50%
			top: 50%
			border: 1px solid #e0e0e0
			border-radius: 50%
			transform-origin: 50% 50%
			img
				box-shadow: none
				border: 0
		.circle-big-bule-0
			position: absolute
			left: 9.6%
			top: 80%
		.circle-big-bule-1
			position: absolute
			right: 0
			top: 50%
			margin-top: -18px
			margin-right: -18px
		.circle-org-0
			position: absolute
			left: 0
			top: 50%
			margin-top: -26px
			margin-left: -29px
		.circle-orange-0
			position: absolute
			left: 50%
			top: 50%
			margin-left: -66px
			margin-top: -26px
			animation: rotateRev 6s linear infinite
		.rotate-circle-0
			margin-top: -635px
			margin-left: -635px
			width: 1270px
			height: 1270px
			animation: rotateRev 6s linear infinite
		.rotate-circle-1
			margin-top: -429px
			margin-left: -429px
			width: 858px
			height: 858px
			animation: rotate 5s linear infinite
		.rotate-circle-2
			margin-top: -265px
			margin-left: -265px
			width: 530px
			height: 530px
			animation: rotateRev 4s linear infinite
		.rotate-circle-3
			margin-top: -127px
			margin-left: -127px
			width: 254px
			height: 254px
			animation: rotate 3s linear infinite
		.rotate-circle-4
			margin-top: -127px
			margin-left: -127px
			width: 254px
			height: 254px
		.circle-orange
			position: absolute
			background-color: #de803b
			opacity: 0.7
			border-radius: 50%
			&.circle-orange-big
				width: 16px
				height: 16px
			&.circle-orange-mid
				width: 13px
				height: 13px
			&.circle-orange-sm
				width: 10px
				height: 10px
			&.circle-orange-left
				top: 50%
				left: 0
				transform: translate(-50%, -50%)
			&.circle-orange-right
				top: 20%
				right: 10%
				transform: translate(50%, -50%)
			&.circle-orange-top
				top: 0
				left: 50%
				transform: translate(-50%, -50%)
			&.circle-orange-bottom
				bottom: 0
				left: 50%
				transform: translate(-50%, 50%)

@keyframes rotate {
	from {
		transform: rotate(0deg)
	}
	to {
		transform: rotate(360deg)
	}
}
@keyframes rotateRev {
	from {
		transform: rotate(360deg)
	}
	to {
		transform: rotate(0deg)
	}
}

.help-docs
